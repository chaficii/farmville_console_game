      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="Incomes">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="HarvestIncome"/>
        <xs:element ref="MoneyIncome">
          <xs:annotation>
            <xs:documentation>45 apples45 apples45 apples45 apples</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element ref="XP_Gain"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="Seed">

      <xs:simpleContent>
        <xs:extension base="xs:byte">
          <xs:attribute type="xs:string" name="CurrencyUnit" use="optional"/>
        </xs:extension>
      </xs:simpleContent>
    </xs:complexType>
  </xs:element>
  <xs:element name="price">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="originalPrice" minOccurs="0"/>
        <xs:element ref="discount" minOccurs="0"/>
        <xs:element ref="shipping" minOccurs="0"/>
        <xs:element ref="finalPrice"/>
      </xs:sequence>
      <xs:attribute type="xs:string" name="CurrencyUnit" use="optional"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="Other_Costs">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="PlantCost" minOccurs="0"/>
        <xs:element ref="WateringCost" minOccurs="0"/>
        <xs:element ref="HarvestCost" minOccurs="0"/>
        <xs:element ref="WateringCardinal" minOccurs="0"/>
        <xs:element ref="WaterCost" minOccurs="0">
          <xs:annotation>
            <xs:documentation>how many times does it need to drink until it provides a producthow many times does it need to drink until it provides a producthow many times does it need to drink until it provides a product</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element ref="UnHarvestSeedCost" minOccurs="0"/>
        <xs:element ref="TreeTrimmingCost" minOccurs="0">
          <xs:annotation>
            <xs:documentation>every timeevery time</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element ref="TreeCutCost" minOccurs="0"/>
        <xs:element ref="FeedCost" minOccurs="0">
          <xs:annotation>
            <xs:documentation>every timeevery timeevery time</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element ref="DemandCost" minOccurs="0"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="image">
    <xs:complexType>
      <xs:simpleContent>
        <xs:extension base="xs:string">
          <xs:attribute type="xs:string" name="source" use="optional"/>

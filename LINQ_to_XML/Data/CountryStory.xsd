        <xs:element ref="OverMaturityTime"/>
        <xs:element ref="edition"/>
        <xs:element ref="textDescription">
          <xs:annotation>
            <xs:documentation>comming soon, limited editioncomming soon, limited editioncomming soon, limited editioncomming soon, limited editioncomming soon, limited editioncomming soon, limited editioncomming soon, limited editioncomming soon, limited editioncomming soon, limited editioncomming soon, limited edition</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element ref="levelToUnlock"/>
      </xs:sequence>

        <xs:extension base="xs:float">
          <xs:attribute type="xs:string" name="CurrencyUnit" use="optional"/>
        </xs:extension>
      </xs:simpleContent>
    </xs:complexType>
  </xs:element>
  <xs:element name="XP_Gain">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="seeding" minOccurs="0"/>

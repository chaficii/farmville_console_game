<xs:schema attributeFormDefault="unqualified" elementFormDefault="qualified" xmlns:xs="http://www.w3.org/2001/XMLSchema">
  <xs:element name="categorie">
    <xs:complexType>
      <xs:simpleContent>
        <xs:extension base="xs:string">
          <xs:attribute type="xs:string" name="type" use="optional"/>
        </xs:extension>
      </xs:simpleContent>
    </xs:complexType>
  </xs:element>
  <xs:element name="TVA">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="categorie" maxOccurs="unbounded" minOccurs="0"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="item">
    <xs:complexType>
      <xs:simpleContent>
        <xs:extension base="xs:string">
          <xs:attribute type="xs:string" name="name" use="optional"/>
        </xs:extension>
      </xs:simpleContent>
    </xs:complexType>
  </xs:element>
  <xs:element name="string-array">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="item" maxOccurs="unbounded" minOccurs="0"/>
      </xs:sequence>
      <xs:attribute type="xs:string" name="name" use="optional"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="LogoV">
    <xs:complexType>
      <xs:simpleContent>
        <xs:extension base="xs:string">
          <xs:attribute type="xs:string" name="source" use="optional"/>
          <xs:attribute type="xs:string" name="size" use="optional"/>
        </xs:extension>
      </xs:simpleContent>
    </xs:complexType>
  </xs:element>
  <xs:element name="font">
    <xs:complexType>
      <xs:simpleContent>
        <xs:extension base="xs:string">
          <xs:attribute type="xs:string" name="type" use="optional"/>
          <xs:attribute type="xs:string" name="nom" use="optional"/>
          <xs:attribute type="xs:byte" name="numero" use="optional"/>
        </xs:extension>
      </xs:simpleContent>
    </xs:complexType>
  </xs:element>
  <xs:element name="nom" type="xs:string"/>
  <xs:element name="Rouge" type="xs:short"/>
  <xs:element name="Vert" type="xs:short"/>
  <xs:element name="Bleu" type="xs:short"/>
  <xs:element name="melangeCouleur">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="nom"/>
        <xs:element ref="Rouge"/>
        <xs:element ref="Vert"/>
        <xs:element ref="Bleu"/>
      </xs:sequence>
      <xs:attribute type="xs:byte" name="numero" use="optional"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="icon">
    <xs:complexType>
      <xs:simpleContent>
        <xs:extension base="xs:string">
          <xs:attribute type="xs:string" name="source" use="optional"/>
          <xs:attribute type="xs:string" name="size" use="optional"/>
          <xs:attribute type="xs:string" name="nom" use="optional"/>
        </xs:extension>
      </xs:simpleContent>
    </xs:complexType>
  </xs:element>
  <xs:element name="logo">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="LogoV" maxOccurs="unbounded" minOccurs="0"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="fontFamily">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="font" maxOccurs="unbounded" minOccurs="0"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="couleur">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="melangeCouleur" maxOccurs="unbounded" minOccurs="0"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="icons">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="icon" maxOccurs="unbounded" minOccurs="0"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="financiere">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="TVA"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="CountryCodes">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="string-array" maxOccurs="unbounded" minOccurs="0">
          <xs:annotation>
            <xs:documentation>country list</xs:documentation>
          </xs:annotation>
        </xs:element>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="infographique">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="logo"/>
        <xs:element ref="fontFamily"/>
        <xs:element ref="couleur"/>
        <xs:element ref="icons"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="name" type="xs:string"/>
  <xs:element name="provider" type="xs:string"/>
  <xs:element name="brand" type="xs:string"/>
  <xs:element name="MaturityTime">

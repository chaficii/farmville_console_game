      <xs:simpleContent>
        <xs:extension base="xs:short">
          <xs:attribute type="xs:string" name="TimeUnit" use="optional"/>
        </xs:extension>
      </xs:simpleContent>
    </xs:complexType>
  </xs:element>
  <xs:element name="OverMaturityTime">
    <xs:complexType>
      <xs:simpleContent>
        <xs:extension base="xs:byte">
          <xs:attribute type="xs:string" name="TimeUnit" use="optional"/>
        </xs:extension>
      </xs:simpleContent>
    </xs:complexType>
  </xs:element>
  <xs:element name="edition">
    <xs:complexType>
      <xs:simpleContent>
        <xs:extension base="xs:string">
          <xs:attribute type="xs:string" name="type" use="optional"/>
        </xs:extension>
      </xs:simpleContent>
    </xs:complexType>
  </xs:element>
  <xs:element name="textDescription" type="xs:string"/>
  <xs:element name="levelToUnlock" type="xs:byte"/>
  <xs:element name="amount" type="xs:byte"/>
  <xs:element name="delayTime" type="xs:float"/>
  <xs:element name="freeShipping" type="xs:string"/>
  <xs:element name="shippingCost">
    <xs:complexType>
      <xs:simpleContent>
        <xs:extension base="xs:byte">
          <xs:attribute type="xs:string" name="CurrencyUnit" use="optional"/>
        </xs:extension>
      </xs:simpleContent>
    </xs:complexType>
  </xs:element>
  <xs:element name="originalPrice" type="xs:short"/>
  <xs:element name="discount">
